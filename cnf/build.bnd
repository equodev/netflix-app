#-include: https://dl.equoplatform.com/framework/develop/equobuild.bnd

-plugin.1.Equo: \
    aQute.bnd.repository.p2.provider.P2Repository; \
        name = Equo; \
        url = "https://dl.equoplatform.com/framework/develop/repo/"; \
        poll.time = -1; \
        cache = ~/.bnd/cache/equo

-plugin.5.Release: \
    aQute.bnd.deployer.repository.LocalIndexedRepo; \
        name = Release; \
        local = ${build}/release; \
        type = R5

-releaserepo: Release
-baselinerepo: Release